server {
	listen 80 default_server;
	listen [::]:80 default_server;

	root /var/www/html;

	index index.html index.htm index.nginx-debian.html;

	server_name _;

	location / {
		try_files $uri $uri/ =404;
	}

	location {{ munki_nginx_url_path }} {
		alias {{ munki_repo }};
		autoindex off;
	}
}
